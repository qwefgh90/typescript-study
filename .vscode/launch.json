{
    // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
    // 기존 특성에 대한 설명을 보려면 가리킵니다.
    // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/built/local/tsc.js",
            "outFiles": [
                "${workspaceFolder}/built/local/*.js"
            ]
            ,"args": ["-p"
                ,"${workspaceFolder}/analysis/serial-task"]
            ,"console": "internalConsole", 
            "outputCapture": "std",
        },
        
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "StringKeyword",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/built/local/tsc.js",
            "outFiles": [
                "${workspaceFolder}/built/local/*.js"
            ]
            ,"args": ["${workspaceFolder}/analysis/parameter.ts"]
            ,"console": "internalConsole", 
            "outputCapture": "std",
        }
    ]
}